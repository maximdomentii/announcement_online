<!DOCTYPE html>
<html>
<head>
  <title>Announcements</title>
  <%= csrf_meta_tags %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
<header class="fixed-header">
  <div class="container">
    <div class="row">
      <div class="col-md-10">
        <div class="title">
          <span>Announcements</span>
        </div>
      </div>
      <div class="col-md-2">
        <div class="loged-in">
          <h4 class="username">Log in</h4>
        </div>
      </div>
    </div>
  </div>
</header>
<div class="container">
  <%= yield %>
</div>
</body>
</html>
